    <decisions>
        <description>Check if the Parent workorder is f ServiceRequest Type and of priority S1.</description>
        <name>Is_WO_S1_Priority_and_SR_type</name>
        <label>Is WO S1 Priority and SR type</label>
        <locationX>578</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrderRecord.FS_WorkOderType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Service Request</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrderRecord.FS_Priority__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>S1</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Mechanic_Inputs</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
