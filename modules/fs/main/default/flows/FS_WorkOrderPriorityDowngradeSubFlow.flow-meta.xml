    <decisions>
        <name>Check_for_Mechanic_Reason_and_TP_flag</name>
        <label>Check for Mechanic Reason and TP flag</label>
        <locationX>314</locationX>
        <locationY>350</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>TP_flag_is_True_and_Reason_is_Need_New_Visit</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Trapped_Passenger_Released_By_OTIS</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Mechanic_Reason</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Need New Visit On Site</stringValue>
                </rightValue>
            </conditions>
            <conditions>
