        <locationX>536</locationX>
        <locationY>477</locationY>
        <collectionReference>Get_Woli_template</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_WOLI_from_WOLI_Template</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>WOLI_Records_Create</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
