        <locationY>48</locationY>
        <connector>
            <targetReference>Get_Woli_template</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>WorkTypeId</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>IsGeneratedFromMaintenancePlan</field>
