            </value>
        </assignmentItems>
        <connector>
            <targetReference>WOLI_Template_iteration</targetReference>
        </connector>
    </assignments>
    <description>This flow retrieves WOLI Template records associated to Work order&apos;s Work type &amp; generates WOLI</description>
    <environments>Default</environments>
    <interviewLabel>WOLI Records Creation {!$Flow.CurrentDateTime}</interviewLabel>
