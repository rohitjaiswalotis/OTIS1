    <formulas>
        <name>IsCompletedWithinTime</name>
        <dataType>Boolean</dataType>
        <expression>IF( AND({!$Record.WorkType.FS_MaxDuration__c}&gt; {!$Record.ActualDuration}, {!$Record.ActualDuration} &gt; {!$Record.WorkType.FS_MinDuration__c}) , True, False)</expression>
    </formulas>
    <formulas>
