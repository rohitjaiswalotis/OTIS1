<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <decisions>
        <description>This decision will check whether  the related work Oder&apos;s record type is maintenance or not</description>
        <name>Check_For_Maintenance</name>
        <label>Check For Maintenance</label>
        <locationX>473</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>Not maintenance</defaultConnectorLabel>
        <rules>
            <name>Maintenance</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.WorkOrder__r.RecordType.Id</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Setup.OTISCustomSettings__c.Work_Order_Maintenance_Record_Type__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Condition_Check</targetReference>
            </connector>
            <label>Maintenance</label>
        </rules>
    </decisions>
    <decisions>
        <name>Condition_Check</name>
        <label>Condition Check</label>
        <locationX>275</locationX>
        <locationY>384</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Till_Now_SA_Is_Not_Complete</name>
            <conditionLogic>1 AND (2 OR 3 OR 4) AND 5 AND 6</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Onsite</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Cannot Complete</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Canceled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.AppxSAEndTime__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>CurrentTime</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.AssignedServiceResource__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Udate_WO_Safety_Check</targetReference>
            </connector>
            <label>Till Now SA Is Not Complete</label>
        </rules>
    </decisions>
    <description>This version of the flow will filter Related Work orders record type ( with Custom Settings)
