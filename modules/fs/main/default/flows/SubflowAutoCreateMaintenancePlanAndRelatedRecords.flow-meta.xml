    <formulas>
        <name>EndDateCalculation</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!ContractLineItemRecord.EndDate}))+{!GetMonthFromEndDate}+{!GetDayFromEndDate}+&apos;T000000Z;&apos;</expression>
    </formulas>
    <formulas>
        <name>GeneralizedMPTitle</name>
        <dataType>String</dataType>
        <expression>{!ContractLineItemRecord.Name__c} + &apos; - &apos; +{!ContractLineItemRecord.Asset.Name} + &apos; - &apos; + &apos; AutoGenerated Maintenance Plan&apos;</expression>
    </formulas>
    <formulas>
        <name>GetDayFromEndDate</name>
        <dataType>String</dataType>
        <expression>IF(LEN(TEXT(DAY({!ContractLineItemRecord.EndDate})))==1, &apos;0&apos;+TEXT(DAY({!ContractLineItemRecord.EndDate})), TEXT(DAY({!ContractLineItemRecord.EndDate})))</expression>
    </formulas>
    <formulas>
        <name>GetMonthFromEndDate</name>
        <dataType>String</dataType>
        <expression>IF(LEN(TEXT(MONTH({!ContractLineItemRecord.EndDate})))==1, &apos;0&apos;+TEXT(MONTH({!ContractLineItemRecord.EndDate})), TEXT(MONTH({!ContractLineItemRecord.EndDate})))</expression>
    </formulas>
