    <decisions>
        <name>Check_For_System_Reason_Priority</name>
        <label>Check For System Reason Priority</label>
        <locationX>578</locationX>
        <locationY>350</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_Prior_Greater_Than_Present</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PriorReasonValue</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>PresentReasonValue</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SystemReason_Update_with_Prior</targetReference>
            </connector>
            <label>If Prior Greater Than Present</label>
        </rules>
        <rules>
            <name>If_Present_Greater_Than_Prior</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PresentReasonValue</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>PriorReasonValue</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>System_Reason_Update_with_Present</targetReference>
            </connector>
            <label>If Present Greater Than Prior</label>
        </rules>
    </decisions>
    <decisions>
        <name>Checking_the_Value_Null_or_Not</name>
        <label>Checking the Value Null or Not</label>
        <locationX>578</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>When_Prior_IsNull</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Prio_Sys_Reason_From_WO</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Prio_Sys_Reason_From_WO</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>System_Reason_Update</targetReference>
            </connector>
            <label>When Prior IsNull</label>
        </rules>
        <rules>
            <name>Is_Not_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Prio_Sys_Reason_From_WO</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Sys_Reason_From_WO</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Priority_Value</targetReference>
            </connector>
            <label>Is Not Null</label>
        </rules>
    </decisions>
    <description>This Flow will Update the System Reason 1field on WO Level ( Without Meta Data)</description>
    <environments>Default</environments>
    <formulas>
        <description>This Formula variable will hold the Value of the Present System Reason Value</description>
        <name>PresentReasonValue</name>
        <dataType>Number</dataType>
        <expression>VALUE(LEFT({!PresentReason} ,FIND(&apos;.&apos;, {!PresentReason}) ))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>This variable will hold the value for the Prior System Reason</description>
        <name>PriorReasonValue</name>
        <dataType>Number</dataType>
        <expression>VALUE(LEFT({!PriorReason} ,FIND(&apos;.&apos; , {!PriorReason}) ))</expression>
        <scale>0</scale>
    </formulas>
