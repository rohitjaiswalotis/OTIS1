<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <decisions>
        <name>Priority_Decision</name>
        <label>Priority Decision</label>
        <locationX>314</locationX>
        <locationY>431</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>S1_and_S2</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrder_Details.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>S1 - Emergency</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrder_Details.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>S2 - High</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Service_Appointment_LTA_Update_to_1_Hr</targetReference>
            </connector>
            <label>S1 and S2</label>
        </rules>
        <rules>
            <name>S3_and_S4</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrder_Details.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>S3 - Normal</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrder_Details.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>S4 - Low</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>No_LTA_Update</targetReference>
            </connector>
            <label>S3 and S4</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>ServiceLTADate</name>
        <dataType>DateTime</dataType>
        <expression>{!ServiceAppoinmentCreatedDate}+(1/24)</expression>
    </formulas>
    <interviewLabel>Service Appointment Pririty Date {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Service Appointment Priority Date</label>
    <processMetadataValues>
